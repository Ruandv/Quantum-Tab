{
  "name": "quantum-tab",
  "version": "1.4.0",
  "description": "New tab. New intelligence.",
  "main": "dist/background.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css,html}",
    "type-check": "tsc --noEmit",
    "pre-release": "npm run type-check && npm run lint && npm run format && npm run build",
    "package": "node ./scripts/create-zip.js",
    "commit": "git-cz",
    "prepare": "husky install || true",
    "docs:sync-wiki": "node scripts/sync-docs-to-wiki.js",
    "docs:sync-wiki-local": "node scripts/local-sync-docs-to-wiki.js"
  },
  "keywords": [
    "chrome-extension",
    "react",
    "typescript",
    "manifest-v3"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/react-beautiful-dnd": "^13.1.8",
    "i18next": "^25.5.2",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.0.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.246",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.3",
    "prettier": "^3.0.3",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
